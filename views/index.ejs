<%-include('./partials/headNav')%>

<section id="all-products">
    <% productsData.forEach(elt => { %>
    <article>
        <a href="/details/<%= elt._id %>"><img src=" <%= elt["Product-picture-Link"] %>" alt="product-photo"></img></a>
        <div class="caption-wrap">
            <ul>
                <li>
                    <a href="/details/<%= elt._id %>"><%= elt["Product-name"] %></a>
                </li>
                <li>
                    <a class="grey" href="/details/<%= elt._id %>"> <%= elt.Company %></a>
                </li>
            </ul>
            <h5>$<%= elt.Price %></h5>
        </div>
    </article>
    <% }) %>
</section>

<footer>THE DESIGN SHOP</footer>
</body>

</html>